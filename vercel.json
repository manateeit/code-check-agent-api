{
  "version": 2,
  "builds": [
    {
      "src": "app/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/main.py"
    }
  ],
  "env": {
    "API_KEY": "@api_key",
    "PERPLEXITY_API_KEY": "@perplexity_api_key",
    "OPENAI_API_KEY": "@openai_api_key",
    "GEMINI_API_KEY": "@gemini_api_key",
    "DEFAULT_LLM_PROVIDER": "openai"
  }
}
